{
  "manifest_version": 2,
  "name": "YouTube Queue Builder",
  "version": "1.0",
  "permissions": ["tabs", "activeTab"],
  "background": {
    "scripts": ["browser-polyfill.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "hello_extensions.png"
  },
  "icons": {
    "16": "hello_extensions.png"
  }
}

